{
  "version": 2,
  "builds": [
    {
      "src": "*/**",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/case-2(.*)",
      "dest": "/index.html",
      "headers": {
        "Content-Type": "text/html"
      }
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    }
  ]
}